{
  "name": "my-react-app",
  "version": "0.0.1",
  "description": "my react application",
  "keywords": [
    "react",
    "typescript"
  ],
  "main": "src/main.tsx",
  "scripts": {
    "postinstall": "typings install",
    "typings": "typings",
    "webpack": "webpack",
    "webpack:w": "webpack -w",
    "webpack:m": "webpack --config webpack.minify.js",
    "server": "browser-sync start -c bs-config.js",
    "build": "tsc && npm run webpack:m",
    "start": "tsc && webpack && concurrently \"npm run webpack:w\" \"npm run server\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test:w": "karma start --browsers PhantomJS",
    "test:f": "karma start --browsers Firefox",
    "test:c": "karma start --browsers Chrome",
    "test:a": "karma start --browsers PhantomJS,Firefox,Chrome",
    "coverage": "rm -fr build ; tsc && mkdir -p coverage && touch coverage/coverage-final.json && karma start coverage.karma.js && remap-istanbul -i coverage/coverage-final.json -t html -o coverage",
    "test": "karma start --single-run --browsers PhantomJS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your/repository.git"
  },
  "bugs": {
    "url": "https://github.com/your/repository/issues"
  },
  "homepage": "https://github.com/your/repository#readme",
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-lodash": "^3.2.9",
    "babel-preset-es2015": "^6.16.0",
    "browser-sync": "^2.17.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "file-loader": "^0.9.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "lodash": "^4.16.4",
    "lodash-webpack-plugin": "^0.10.2",
    "mocha": "^3.1.1",
    "phantomjs-prebuilt": "^2.1.13",
    "react-addons-test-utils": "^15.3.2",
    "remap-istanbul": "^0.6.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.0",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  }
}
